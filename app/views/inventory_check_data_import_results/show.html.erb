<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.inventory_shelf_barcode_import_result')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <b><%= t('activerecord.models.resource_import_file') %>:</b>
  <%= @inventory_shelf_barcode_import_result.inventory_shelf_barcode_import_file.inventory_shelf_barcode_import_file_name %>
</p>

<p>
  <b><%= t('activerecord.attributes.inventory_shelf_barcode_import_result.error_msg') %>:</b>
  <%= h(@inventory_shelf_barcode_import_result.error_msg) if @inventory_shelf_barcode_import_result.error_msg %>
</p>

