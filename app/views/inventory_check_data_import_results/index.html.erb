<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.listing', :model => t('activerecord.models.inventory_check_data_import_result')) -%></h1>
<div id="content_list">
<div style="color: green"><%= flash[:message] -%></div>
  <% if @inventory_check_data_import_file %>
    <h2 class="resource_title">
      <%= link_to @inventory_check_data_import_file.inventory_check_data_import_file_name, @inventory_check_data_import_file %>
    </h2>
  <% end %>

<%= t('activerecord.attributes.resource_import_textresult.imported_row') -%>: <%= @results_num if @results_num -%><br />
<table class="index">
  <tr>
    <th><%= t('activerecord.attributes.resource_import_textresult.body') %></th>
    <th><%= t('activerecord.attributes.resource_import_textresult.error_msg') %></th>
  </tr>

<% @inventory_check_data_import_results.each do |r| %>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= r.body %> </td>
    <td><%= raw r.error_msg %></td>
  </tr>
<% end %>
</table>


</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.back'), @inventory_check_data_import_file -%></li>
  </ul>
</div>
